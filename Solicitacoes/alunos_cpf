/*LISTA DE ALUNOS DE ESCOLA E ANO/SERIE ESPECÍFICOS COM INFORMAÇÃO DE CPF.
"a unidade EMEF CEU Cidade Dutra EOL 019263, solicita o CPF dos alunos do 1º ao 5º ano 
para enviar os dados para acesso as obras digitais do PNLD."
*/
DROP TABLE INTO ##ALU_UE019263_1A5ANOS
SELECT 
DRE,
B.CD_ESCOLA,
SG_TP_ESCOLA,
NOMESC,
A.CD_ALUNO,
A.NM_ALUNO,
A.SG_SERIE_ENSINO,
C.cd_cpf_aluno

INTO ##ALU_UE019263_1A5ANOS
FROM SME_ALUNOS_DIARIO A
JOIN SME_ESCOLA_DIARIO B ON A.CD_ESCOLA=B.CD_ESCOLA
JOIN D_ALUNO C ON A.CD_ALUNO=C.CD_ALUNO

WHERE A.CD_ESCOLA = 019263 AND CD_SERIE_ENSINO IN (84,86,88,232,235,236)
--SELECT * FROM ##ALU_UE019263_1A5ANOS
