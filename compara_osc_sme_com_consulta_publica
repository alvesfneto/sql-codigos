/*
Lista de Entidades - OSC - CONSULTA PÚBLICA, INDICANDO AS QUE TEM PARCEIRA COM A PMSP-SME,
-->TABELA CONSULTA_PUBLICA_OSC RECEBIDO DE COGED
-->TABELA EOL_OSC OBTIDA DA TELA DO EOL --> FUNCIONAL-->ESCOLA--> CONVENIOS-->INSTITUICOES-->RELATÓRIOS OSC-->GERAR EXCEL
*/

DROP TABLE ##COMPARA_CONSULTA_PUBLICA_OSC_SME
SELECT 
A.*,
[Código OSC] AS EOL_OSC,
[Portaria Credenciamento] AS PORTARIA,
[Atividade Credenciada] AS ATIV_CREDENCIADA,
[Situação Documentos] AS SITUACAO,
[Total de parcerias] AS TOT_PARCERIAS,
[Nro# de Parcerias Ativas] AS PARC_ATIVAS
INTO ##COMPARA_CONSULTA_PUBLICA_OSC_SME
FROM CONSULTA_PUBLICA_OSC A
LEFT JOIN OSC_SME B ON A.CNPJ=B.CNPJ
--SELECT * FROM ##COMPARA_CONSULTA_PUBLICA_OSC_SME
-------------------------------------------------------
DROP TABLE ##CONSULTA_PUBLICA_VS_OSC_SME
SELECT * 
INTO ##CONSULTA_PUBLICA_VS_OSC_SME 
FROM ##COMPARA_CONSULTA_PUBLICA_OSC_SME 
WHERE EOL_OSC IS NOT NULL
--SELECT * FROM ##CONSULTA_PUBLICA_VS_OSC_SME
-------------------------------------------------------
